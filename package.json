{
    "name": "@slimio/github",
    "version": "0.2.0",
    "description": "Download repository from github",
    "main": "index.js",
    "scripts": {
        "prepublishOnly": "pkg-ok",
        "test": "ava --verbose",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
        "coverage": "nyc ava",
        "report": "nyc report --reporter=html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SlimIO/github-download.git"
    },
    "keywords": [
        "SlimIO",
        "monitoring"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SlimIO/github-download/issues"
    },
    "homepage": "https://github.com/SlimIO/github-download#readme",
    "dependencies": {
        "@slimio/is": "^1.4.0",
        "extract-zip": "^1.6.7",
        "got": "^9.5.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^7.2.1",
        "@commitlint/config-conventional": "^7.1.2",
        "@escommunity/minami": "^1.0.0",
        "@slimio/eslint-config": "^2.0.3",
        "@types/node": "^10.12.12",
        "ava": "^1.0.0-rc.2",
        "cross-env": "^5.2.0",
        "dotenv": "^6.2.0",
        "eslint": "^5.10.0",
        "husky": "^1.2.0",
        "jsdoc": "^3.5.5",
        "nyc": "^13.1.0",
        "pkg-ok": "^2.3.1",
        "rimraf": "^2.6.2"
    },
    "engines": {
        "node": ">=10"
    },
    "husky": {
        "hooks": {
            "pre-push": "npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "nyc": {
        "check-coverage": true,
        "per-file": false,
        "lines": 98,
        "statements": 98,
        "functions": 98,
        "branches": 90
    }
}

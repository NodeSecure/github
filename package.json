{
  "name": "@slimio/github",
  "version": "0.5.0",
  "description": "Download repository from github",
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "pkg-ok",
    "test": "cross-env psp && ava --verbose test/test.js",
    "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=html",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SlimIO/github-download.git"
  },
  "keywords": [
    "SlimIO",
    "github",
    "download",
    "archive"
  ],
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "author": "SlimIO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SlimIO/github-download/issues"
  },
  "homepage": "https://github.com/SlimIO/github-download#readme",
  "dependencies": {
    "@slimio/is": "^1.5.0",
    "follow-redirects": "^1.9.0",
    "tar-fs": "^2.0.0",
    "snyk": "^1.316.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@escommunity/minami": "^1.0.0",
    "@slimio/eslint-config": "^3.0.3",
    "@slimio/psp": "^0.9.2",
    "ava": "^2.4.0",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^6.6.0",
    "husky": "^3.0.9",
    "jsdoc": "^3.6.1",
    "nyc": "^14.1.1",
    "pkg-ok": "^2.3.1"
  },
  "engines": {
    "node": ">=12"
  },
  "husky": {
    "hooks": {
      "pre-push": "cross-env eslint index.js && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 98,
    "statements": 98,
    "functions": 98,
    "branches": 90
  },
  "snyk": true
}
